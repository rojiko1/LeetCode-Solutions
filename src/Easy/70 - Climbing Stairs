class Workspace.Solution {

    public int climbStairs(int n) {
        if (n == 1) {
            return 1;
        }
        else if (n == 2) {
            return 2;
        }
        else {
            int temp;
            int first = 1;
            int second = 2;
            for (int i = 3; i < n; i++) {
                temp = second;
                second += first;
                first = temp;
            }
            return first + second;
        }
    }

}